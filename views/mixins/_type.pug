mixin type(type)
  .vegetable
    img.vegetable__artwork(src=`/uploads/types/${type.photo  || 'default-type.jpg'}`)
    .vegetable__details
      .vegetable__title
        h2= type.name
        .vegetable__vote
          p 👍 👎
      .vegetable__desc
        p= type.description
      .vegetable__months
        .vegetable__months--sow 
          p= type.Sow
        .vegetable__months--harvest 
          p= type.Harvest

  